{
  "name": "perruchaApp",
  "version": "0.0.1",
  "description": "perrucha App",
  "main": "src/index.js",
  "scripts": {
    "test-local": "node node_modules/cucumber/bin/cucumber.js --tags @mandatory --tags @local test/bdd/features",
    "test": "node node_modules/cucumber/bin/cucumber.js --tags @mandatory --tags @release test/bdd/features",
    "mock": "node node_modules/json-server/bin/index.js --watch test/mocks/db.json",
    "watch": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./scripts/webpack.config.js --colors --no-info  --env.target='dev' --env.env='loc' --env.platform='web' --env.version='0.0.1' --env.languaje='EN'",
    "fail-watch": "node node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'test' -- node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./scripts/webpack.config.js --colors --no-info  --env.target='dev' --env.env='loc' --env.platform='web' --env.version='0.0.1' --env.languaje='EN'",
    "dev": "node node_modules/webpack/bin/webpack.js --config ./scripts/webpack.config.js --progress --profile --colors --env.target='dev' --env.env='dev' --env.platform='web' --env.version='0.0.1' --env.languaje='EN'",
    "build": "node node_modules/webpack/bin/webpack.js --config ./scripts/webpack.config.js --progress --profile --colors --env.target='prod' --env.env='prod' --env.platform='web' --env.version='0.0.1' --env.languaje='EN'"
  },
  "author": "dokoto.moloko@gmail.com",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "es6-promise": "^4.1.0",
    "file-loader": "^0.11.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "babel-runtime": "^6.18.0",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.2.0",
    "vue-router": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue-app": "^1.1.1",
    "chromedriver": "^2.31.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^2.0.0",
    "css-loader": "^0.28.0",
    "cucumber": "^2.3.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.12.0",
    "log-update": "^2.0.0",
    "node-sass": "^4.5.3",
    "offline-plugin": "^4.8.4",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "selenium-webdriver": "^3.4.0",
    "stats-webpack-plugin": "^0.6.0",
